.landingTitles
	font-family "Source Serif Pro"
	font-size 1.5em
	font-weight 500
	color #000000
	margin 20px auto 15px auto
	padding 0px
	display block
	text-align center
	
.bigTitlesSS
	font-family "Source Serif Pro"
	font-size 1.4em
	font-weight normal
	color #000000
	margin 20px auto 5px auto
	padding 0px
	display block
	text-align center
	
.bigTitlesOS
	font-family "Open Sans"
	font-size 1.3em
	font-weight normal
	color #000000
	margin 20px auto 20px auto
	padding 0px
	display block
	text-align center
	
	
.smallContent
	font-family "Open Sans"
	font-size 0.8em
	font-weight lighter
	color #000000
	margin 0px auto 0px auto
	padding 0px
	display block
	text-align center
	
.mediumContent
	font-family "Open Sans"
	font-size 1.1em
	font-weight lighter
	color #000000
	margin 0px auto 0px auto
	padding 0px
	display block
	text-align center
	
.bigContent
	font-family "Open Sans"
	font-size 1.2em
	font-weight lighter
	color #000000
	margin 0px auto 0px auto
	padding 0px
	display block
	text-align center
	
.underlineBlue
	height 2px
	width 35px
	margin 0px auto
	background-color #007EFF
	
.underlineGrey
	height 1px
	width 170px
	margin 0px auto
	background-color #D0D0D0
	
@media screen and (min-width: 1180px)
	.landingTitles
		font-size 1.8em
		
	.bigTitlesOS
		font-size 1em
		
	.mediumContent
		font-size 0.8em
		
	.smallContent
		font-size 0.8em
		
	.bigTitlesSS
		font-size 1.2em
		text-align left